[
  {
    "kernelSource": {
      "name": "clover",
      "repo": "https://github.com/pix106/android_kernel_xiaomi_southwest-4.19",
      "branch": "main",
      "device": "clover",
      "defconfig": "clover_defconfig"
    },
    "toolchains": [
      {
        "name": "clang-r383902",
        "repo": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86",
        "branch": "master-kernel-build-2021",
        "binaryEnv": ["clang-r383902/bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
